@using Cim.Web.Controllers.Surface
@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = null;
}

<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@Umbraco.Field("title")</title>
    <link rel="icon" href="/images/CIM-favicon-16.ico" sizes="16x16" type="image/x-icon">
    <link rel="icon" href="/images/CIM-favicon-32.ico" sizes="32x32" type="image/x-icon">
    <link rel="stylesheet" href="/css/app.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/Swiper/4.3.5/css/swiper.min.css">
    <link rel="stylesheet" href="/css/style.css" type="text/css" media="all" />
    <link rel="stylesheet" href="/css/fontawesome-pro-5.3.1/css/all.css" type="text/css" media="all" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Swiper/4.3.5/js/swiper.min.js"></script>
</head>
<body>
    <div class="body-layover"></div><!-- /.layover -->
    <div class="body-layover-nav"></div><!-- /.layover -->
    @(Html.Action<UsersController>("Login"))
    @(Html.Action<SharedController>("Header"))
    @RenderBody()
    @(Html.Action<SharedController>("Footer"))
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/waypoints/4.0.1/jquery.waypoints.min.js"></script>
    <script src="/js/main.js"></script>
    <script src="/js/jquery-ui.min.js" type="text/javascript"></script>
    <script src="/js/function.js" type="text/javascript"></script>
    <script src="/js/strength.js" type="text/javascript"></script>
</body>
</html>

